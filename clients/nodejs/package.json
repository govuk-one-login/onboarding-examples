{
  "name": "gov-uk-one-login-rp",
  "version": "1.0.0",
  "description": "Example GOV.UK One Login Relying Party in Typescript",
  "private": true,
  "main": "dist/index.js",
  "repository": {
    "url": "github:govuk-one-login/rp-reference",
    "directory": "clients/nodejs"
  },
  "bugs": {
    "url": "https://github.com/govuk-one-login/rp-reference/issues"
  },
  "scripts": {
    "build": "npx ts-node -r dotenv/config ./scripts/build.ts",
    "dev": "nodemon --exec \"npx ts-node -r dotenv/config ./src/index.ts dotenv_config_path=./.env dotenv_config_debug=true\"",
    "simulator": "curl -s localhost:3000/.well-known/openid-configuration | jq .",
    "simulator:logs": "docker logs $(docker ps | grep gdsdougapd/simulator:latest | gsed -E 's/[\t ]+.*$//') | gsed 1,3d | pino-pretty",
    "simulator:shell": "docker exec -ti $(docker ps | grep gdsdougapd/simulator:latest | gsed -E 's/[\t ]+.*$//') bash",
    "simulator:status": "docker ps | grep gdsdougapd/simulator:latest",
    "simulator:start": "curl -s localhost:3000 > /dev/null || docker run --rm -d -p 3000:3000 gdsdougapd/simulator:latest",
    "simulator:stop": "docker kill $(docker ps | grep gdsdougapd/simulator:latest | gsed -E 's/[\t ]+.*$$//')",
    "simulator:jwks": "curl -s localhost:3000/.well-known/jwks.json | jq .",
    "start": "node -r dotenv/config dist/index.js dotenv_config_path=./.env dotenv_config_debug=true"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=16.0.0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@tsconfig/node16": "^1.0.2",
    "@types/chromedriver": "^81.0.1",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.7",
    "@types/node": "^16.18.91",
    "@types/nunjucks": "^3.2.4",
    "@types/pino-http": "^5.8.4",
    "dotenv": "^16.0.0",
    "nodemon": "^2.0.15",
    "pino-pretty": "^9.1.1",
    "ts-node": "^10.7.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "did-resolver": "^4.1.0",
    "express": "^4.18.3",
    "express-session": "^1.17.3",
    "express-pino-logger": "^7.0.0",
    "govuk-frontend": "^4.0.1",
    "jose": "^4.15.5",
    "nunjucks": "^3.2.4",
    "openid-client": "^5.1.5",
    "pino": "^9.4.0",
    "pino-http": "^9.0.0",
    "sass": "^1.63.6",
    "web-did-resolver": "^2.0.27"
  }
}
